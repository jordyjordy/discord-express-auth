{
  "name": "@jordyjordy/discord-express-auth",
  "version": "0.1.3",
  "description": "A basic package to use discord oauth2 to authenticate users in express",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "npx tsc --p ./tsconfig.build.json",
    "start": "tsc && node dist/index.js",
    "deploy": "node dist/index.js"
  },
  "author": "J. van der Tang",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.12.0",
    "discord.js-typings": "github:discordjs/discord.js-typings",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "jest-mock-axios": "^4.7.1",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.13",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "types": "./dist\\index.d.ts"
}
